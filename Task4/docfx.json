{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Inventory.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "api/**.yml",
          "api/**.md"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "template": [
      "default"
    ]
  }
}